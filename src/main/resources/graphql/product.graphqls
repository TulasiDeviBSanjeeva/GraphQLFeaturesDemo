type Product {
    id: ID!
    name: String!,
    price: Float!,
    realPrice:String!
}

input ProductInput {
    id:ID,
    name:String!
    price: Float,
}

extend type Query {

    product(id:ID!):Product
    products:[Product]!

}

extend type Mutation {

    saveProduct(product:ProductInput!):Product!
    deleteproduct(id:ID!):Boolean!

}